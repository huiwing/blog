<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huiwings.blog.mapper.ArticleMapper">
    <resultMap type="com.huiwings.blog.entity.ArticleEntity" id="ArticleResultMap">
        <id property="id" column="aid"/>
        <result property="title" column="title"/>
        <result property="content" column="content"/>
        <result property="author" column="author"/>
        <result property="typeCode" column="type_code"/>
        <result property="ct" column="create_time"/>
        <result property="ts" column="ts"/>
        <result property="likes" column="c_like"/>
    </resultMap>
</mapper>